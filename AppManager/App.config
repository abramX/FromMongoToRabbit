<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler,Log4net" />
  </configSections>
  <log4net>
    <appender name="TestAppender" type="log4net.Appender.RollingFileAppender">
      <file value="C:\Users\camara\source\repos\FromMongoToRabbit\FromMongoToRabbitLog.log" />
      <encoding value="utf-8" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <!--<rollingStyle value="Size" />
      <maxSizeRollBackups value="5" />
      <maximumFileSize value="5MB" />
      <staticLogFileName value="true" />-->
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %level [%thread] %type.%method - %message%n" />
      </layout>
    </appender>
    <root>
      <level value="All" />
      <!-- If the following line is not included the log file 
      will not be created even if log4net is configured with this file. -->
      <appender-ref ref="TestAppender" />
    </root>
  </log4net>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
  </startup>
  <connectionStrings>
    <add name="MongoConnectionSender" connectionString="mongodb://localhost:27017/mydb" />
    <add name="MongoConnectionReceiver" connectionString="mongodb://localhost:27017/mydb_received" />
  </connectionStrings>
  <appSettings>
    <add key="cronPublicationExpression" value="0 0/1 * * * ?" />
    <add key="MongoDbCollection" value="products" />
    <add key="MongoDbCollectionConsumer" value="products_received" />
    <add key="RabbitHostName" value="localhost" />
    <add key="RabbitUserName" value="guest" />
    <add key="RabbitPassword" value="guest" />
    <add key="RabbitExchangeName" value="IbraExchange" />
    <add key="RabbitQueueName" value="FirstQueueIbra" />
    <add key="ClientSettingsProvider.ServiceUri" value="" />
  </appSettings>
  <runtime>
